{
  "name": "mdast-util-phrasing",
  "version": "0.0.1",
  "description": "Check if a node is a phrasing element",
  "license": "MIT",
  "keywords": [
    "mdast",
    "remark",
    "markdown",
    "phrasing"
  ],
  "repository": "syntax-tree/mdast-util-phrasing",
  "bugs": "https://github.com/syntax-tree/mdast-util-phrasing/issues",
  "author": "Victor Felder <victor@draft.li> (https://draft.li)",
  "contributors": [
    "Victor Felder <victor@draft.li> (https://draft.li)",
    "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)"
  ],
  "files": [
    "index.js"
  ],
  "dependencies": {
    "unist-util-is": "^2.1.1"
  },
  "devDependencies": {
    "remark-cli": "^4.0.0",
    "remark-preset-wooorm": "^3.0.0",
    "nyc": "^11.0.0",
    "tape": "^4.4.0",
    "xo": "^0.18.1"
  },
  "scripts": {
    "build-md": "remark . --quiet --frail --output",
    "build": "npm run build-md",
    "lint": "xo",
    "test-api": "node test.js",
    "test-coverage": "nyc --reporter lcov tape test.js",
    "pretest": "npm run lint",
    "test": "node test.js",
    "posttest": "npm run test-coverage"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 100,
    "functions": 100,
    "branches": 100
  },
  "xo": {
    "space": true,
    "esnext": false,
    "rules": {
      "no-multi-assign": "off"
    }
  },
  "remarkConfig": {
    "plugins": [
      "preset-wooorm"
    ]
  }
}
